{
    "name": "framgia/laravel-test-examples",
    "description": "Unit and integration tests example project.",
    "keywords": ["framework", "laravel", "testing", "phpunit"],
    "license": "MIT",
    "type": "project",
    "homepage": "https://github.com/framgia/laravel-test-examples",
    "authors": [
        {
            "name": "Roman Kinyakin",
            "email": "1@grep.su"
        }
    ],
    "support": {
        "issues": "https://github.com/framgia/laravel-test-examples/issues",
        "source": "https://github.com/framgia/laravel-test-examples",
        "docs": "https://github.com/framgia/laravel-test-guidelines"
    },
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.0-alpha1@dev",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
